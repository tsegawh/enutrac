{
  "name": "enutrac-backend",
  "version": "1.0.0",
  "description": "Backend API for Enutrac subscription management",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec tsx src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    
    "db:push": "prisma db push",
    "db:seed": "tsx src/prisma/seed.ts",
    "db:studio": "prisma studio",
    "seed": "PRISMA_CLIENT_ENGINE_TYPE=wasm prisma db seed",
  "generate": "PRISMA_CLIENT_ENGINE_TYPE=wasm prisma generate"
    "prod": "npm run build && NODE_ENV=production node dist/server.js"
  },
  "prisma": {
    "seed": "tsx src/prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^8.1.0",
    "googleapis": "^164.1.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pdfkit": "^0.17.2",
    "socket.io": "^4.7.4",
    "stripe": "^19.1.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.23",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.4",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/pdfkit": "^0.17.3",
    "@types/stripe": "^8.0.416",
    "@types/ws": "^8.5.10",
    "nodemon": "^3.0.2",
    "prisma": "^5.22.0",
    "tsx": "^4.20.4",
    "typescript": "^5.3.3"
  }
}
